                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="72.0dp"
                android:id="@+id/linearLayout3"
                android:layout_marginLeft="18.0dp"
                android:layout_marginRight="18.0dp">
