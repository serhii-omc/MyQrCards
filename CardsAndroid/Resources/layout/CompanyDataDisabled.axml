                    <ImageView
                        android:background="@drawable/orange_lock"
                        android:layout_width="12.5dp"
                        android:layout_height="19.0dp"
                        android:id="@+id/company_addit_phone_lockIV"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="12.5dp" />
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="72.0dp"
                android:id="@+id/linearLayout3"
                android:layout_marginLeft="18.0dp"
                android:layout_marginRight="18.0dp">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="72.0dp"
                    android:id="@+id/linearLayout3"
                    android:layout_weight="0.7">
                    <android.support.design.widget.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="72.0dp"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:id="@+id/textInputLayout1"
                        android:theme="@style/TextInputStyleDisabled">
                        <android.support.design.widget.TextInputEditText
                            android:importantForAutofill="noExcludeDescendants"
